<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xzit.dao.RoleDao" >
    <resultMap id="baseRoleModelMap" type="com.xzit.entity.model.RoleModel">
        <id column="id" property="id"></id>
        <result column="role_name" property="roleName"></result>
        <result column="intro" property="intro"></result>
        <result column="created_time" property="createdTime"></result>
        <result column="createdUser" property="createdUser"></result>
        <result column="edited_time" property="edited_time"></result>
        <result column="editedUser" property="editedUser"></result>
    </resultMap>
    <!--
        获取所有角色getAllRoles
    -->
    <select id="getAllRoles" resultMap="baseRoleModelMap">



    </select>
</mapper>